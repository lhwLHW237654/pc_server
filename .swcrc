{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript"
    },
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "#/*": ["*"]
    }
  },
  "module": {
    "type": "es6",
    "resolveFully": true
  },
  "exclude": ["node_modules", "uploads", "backup", "build", "logs"]
}
